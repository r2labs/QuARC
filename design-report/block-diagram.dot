digraph blockdiagram {
    concentrate=true;
    node [shape=box] {
        ifc [label="IFC"]
        nucleo [label="Nucleo"]
        terminal [label="User Terminal"]
        user [label="User"]
        arm [label="AL5D"]
    }

    user -> terminal
    terminal -> {user, ifc}

    ifc -> {nucleo, terminal}

    nucleo -> arm
}